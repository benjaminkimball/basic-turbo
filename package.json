  "bugs": {
    "url": "https://github.com/benjaminkimball/vril/issues"
  },
  "homepage": "https://github.com/benjaminkimball/vril#readme",
  "engines": {
    "node": "20.x",
    "npm": "10.x"
  },
  "volta": {
    "node": "20.11.0",
    "npm": "10.2.5"
  },
  "workspaces": [
    "apps/*",
    "packages/*"
